<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="css/admin.css">
    <title>QuizME - Admin</title>
</head>
<body>
    <div class="container">

        <h3>QuizME Admin Console</h3>

        <div class="box">
           <div class="row">
               <label>All quizzes</label>
               <a href="/new_quiz">  <button class="add_quiz_btn">Add Quiz</button>  </a>
           </div>
           <div class="quiz_list">


            <%  for(var i=0; i<quizzes.length; i++){ %> 
            <div class="item">
                <label> <%=quizzes[i].quiz_name%> </label>

                <a href="/view_submissions?quiz=<%=quizzes[i].quiz_name%>" style="text-decoration: none;" >
                 <button>View submissions</button> 
                </a>

            </div>
            <% } %> 

               
           </div>
        </div>

    </div>

    <script>
        
    </script>
</body>
</html>